# Vetria Project - Production Environment Variables
# Copy this to .env on your production server

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_MODEL=gpt-4o
OPENAI_MAX_TOKENS=2000
OPENAI_TEMPERATURE=0.7

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
DOCKER_ENV=true

# Logging
LOG_LEVEL=INFO

# Production Settings
PYTHONUNBUFFERED=1
PYTHONPATH=/app
